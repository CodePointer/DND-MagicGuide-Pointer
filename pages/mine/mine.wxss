/* pages/mine/mine.wxss */
.page-container {
  margin: var(--space-md);
  display: flex;
  flex-direction: column;
  height: 86vh;
}
.loading-containter {
  flex: 1;
  display: flex;
  align-self: center;
}
.spell-list-container {
  flex: 1;
  overflow-y: auto;
}
.footer-horizontal {
  flex-direction: row-reverse !important;
  gap: var(--space-md);
  justify-content: center !important;
  align-items: center;
  flex-wrap: wrap;
}